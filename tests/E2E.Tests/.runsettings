<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <Playwright>
    <BrowserName>chromium</BrowserName>
    <LaunchOptions>
      <Headless>true</Headless>
    </LaunchOptions>
    <ContextOptions>
      <RecordVideoDir>videos/</RecordVideoDir>
      <TracingMode>RetainOnFailure</TracingMode>
    </ContextOptions>
  </Playwright>
</RunSettings>
