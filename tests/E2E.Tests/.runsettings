<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <Playwright>
    <BrowserName>chromium</BrowserName>
    <LaunchOptions>
      <Headless>true</Headless>
    </LaunchOptions>
    <ContextOptions>
      <RecordVideoDir>videos/</RecordVideoDir>
      <RecordVideoSize>1280, 720</RecordVideoSize>
      <ScreenshotOnFailure>true</ScreenshotOnFailure>
    </ContextOptions>
    <Tracing>
      <Snapshots>true</Snapshots>
      <Screenshots>true</Screenshots>
      <Sources>true</Sources>
    </Tracing>
  </Playwright>
</RunSettings>
